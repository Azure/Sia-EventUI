{
  "name": "SIA",
  "description": "SRE Incident Assistant",
  "version": "0.0.0.3",
  "manifest_version": 2,
  "browser_action": {
    "content_scripts" : [{
      "matches": [],
      "js":["ticketHook.js"]
    }],
    "default_popup": "/extension.html"
    },
    "background": {
      "scripts":["messager.js"]
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "author": "microsoft",
    "permissions": [
      "identity",
      "tabs",
      "https://*/"
    ]
}
